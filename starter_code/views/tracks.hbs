<main class="tracks-container">

    <div>
        <p>Title</p>
        <p>Listen</p>
    </div>

    <div class="track">
        {{#each tracksObject}}
                {{#if this.preview_url}}
                    <p>{{this.name}}</p>

                    <audio 
                                controls
                                src="{{this.preview_url}}">
                    </audio>
                {{else}}
            {{/if}}
        {{/each}}
    </div>

</main>